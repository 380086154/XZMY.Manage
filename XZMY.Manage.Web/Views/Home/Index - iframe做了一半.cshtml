    <script type="text/javascript">

        var cookieName = 'menu_active_name_1';
        var html = $('#liTemplate').html();

        var bindChild = function(data, id) {
            var li = [];
            $.each(data, function(index, item) {
                if (item.pId == id) {
                    li.push('<li id="_ID_"><a onclick="redirect(\'_ID_\',\'_PID_\',\'_URL_\');" href="javascript:void(0);">_NAME_</a></li>'.replace(/_ID_/ig, item.id).replace('_URL_', item.Url).replace('_NAME_', item.name).replace(/_PID_/ig, id).replace(/-/ig, '_'));
                }
            });
            return li.join('');
        };

        var loadMenu = function () {
            var value = $.cookie(cookieName);
            var idList = [];
            if (value != undefined) idList = value.split('|');

            //alert(value);

            $.ajax({
                type: 'POST',
                url: '/home/AjaxMenuList',
                dataType: 'JSON',
                success: function(result) {
                    if (result.success) {

                        var data = result.rows;
                        var li = [];

                        $.each(data, function(index, item) {

                            if (item.pId == '00000000-0000-0000-0000-000000000000') {
                                li.push(html.replace(/__ID__/ig, item.id.replace(/-/ig, '_'))
                                    .replace('__NAME__', item.name)
                                    .replace('__LI__', bindChild(data, item.id)));
                            }
                        });

                        $(li.join('')).appendTo($('#ulMenu'));

                        if (idList[0] != undefined && idList[0].length > 0) $('#' + idList[0]).addClass('active');
                        if (idList[1] != undefined && idList[1].length > 0) $('#' + idList[1]).addClass('active');
                    }
                },
                error: function(XMLHttpRequest, textStatus, errorThrown) {
                    alert(JSON.stringify(errorThrown));
                },
                async: false
            });
        };

        var redirect = function (id, pid, url) {
            var v = id + '|' + pid;
            //alert(v);
            $.cookie(cookieName, v, { expires: 1024 });

            //location.href = url;
            $('#mainFrame').attr("src", url);
        };

        jQuery(document).ready(function() {
            App.init(); // initlayout and core plugins
            //Index.init();
